{
    "framework": "vite",
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "installCommand": "npm install",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://api.audioloop.com.br/api/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}